                                android:text="审批状态"
                                android:layout_toRightOf="@id/tv_state_title"
                                android:textSize="@dimen/dp_14"
                                android:textColor="@color/black"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content" />
                        </RelativeLayout>

