<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:id="@+id/rl_title"
        android:layout_width="match_parent"
        android:background="#ffffff"
        android:gravity="center_vertical"
        android:layout_height="@dimen/dp_45">
        <ImageView
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_width="@dimen/dp_15"
            android:id="@+id/iv_back"
            android:layout_centerVertical="true"
            android:background="@mipmap/ic_left"
            android:layout_height="@dimen/dp_15" />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="会议信息"
            android:textColor="#313131"
            android:textSize="@dimen/sp_17" />
    </RelativeLayout>
    <TextView
        android:visibility="gone"
        android:layout_width="match_parent"
        android:background="@color/color_xian"
        android:layout_height="@dimen/dp_0_5"></TextView>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:orientation="vertical"
        android:layout_marginTop="@dimen/dp_15"
        android:layout_marginLeft="@dimen/dp_10"
        android:layout_marginRight="@dimen/dp_10"
        android:paddingBottom="@dimen/dp_20"
        android:layout_marginBottom="@dimen/dp_20"
        android:background="@color/colorwhite"
        >
        <LinearLayout
            android:padding="@dimen/dp_10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            >
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_35"
                android:layout_marginTop="@dimen/dp_5"
                android:layout_marginBottom="@dimen/dp_5"
                >
                <TextView
                    android:gravity="center"
                    android:id="@+id/tv_title"
                    android:text="会议标题"
                    android:textColor="#313131"
                    android:drawablePadding="@dimen/dp_3"
                    android:drawableLeft="@mipmap/qj_leibie_icon_qgl"
                    android:textSize="@dimen/dp_14"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    />
                <TextView
                    android:id="@+id/et_title"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:textColor="@color/black"
                    android:textSize="@dimen/dp_14"
                    android:background="@null"
                    android:gravity="center_vertical"
                    android:layout_toRightOf="@id/tv_title"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    />
            </RelativeLayout>
            <View
                android:background="@color/colorText"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_0_5"/>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_35"
                android:focusable="true"
                android:focusableInTouchMode="true"
                android:layout_marginTop="@dimen/dp_5"
                >
                <TextView
                    android:id="@+id/tv_title0"
                    android:text="签到类型"
                    android:layout_marginLeft="@dimen/dp_2"
                    android:textColor="#313131"
                    android:drawablePadding="@dimen/dp_5"
                    android:drawableLeft="@mipmap/meeting_check_icon"
                    android:textSize="@dimen/dp_14"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    />
                <TextView
                    android:id="@+id/et_signType"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:textColor="@color/black"
                    android:textSize="@dimen/dp_14"
                    android:background="@null"
                    android:gravity="center_vertical"
                    android:layout_toRightOf="@id/tv_title0"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    />
            </RelativeLayout>
            <View
                android:background="@color/colorText"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_0_5"/>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_35"
                android:layout_marginTop="@dimen/dp_5"
                android:layout_marginBottom="@dimen/dp_5"
                >
                <TextView
                    android:gravity="center"
                    android:id="@+id/tv_part_time_title"
                    android:text="开始时间"
                    android:textColor="#313131"
                    android:textSize="@dimen/dp_14"
                    android:layout_width="wrap_content"
                    android:drawablePadding="@dimen/dp_3"
                    android:drawableLeft="@mipmap/qj_kaishi_time_icon"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    />
                <TextView
                    android:id="@+id/tv_part_time"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:textColor="@color/black"
                    android:textSize="@dimen/dp_14"
                    android:background="@null"
                    android:gravity="center_vertical"
                    android:layout_toRightOf="@id/tv_part_time_title"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    />
            </RelativeLayout>
            <View
                android:background="@color/colorText"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_0_5"/>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_35"
                android:layout_marginTop="@dimen/dp_5"
                android:layout_marginBottom="@dimen/dp_5"
                >
                <TextView
                    android:gravity="center"
                    android:id="@+id/tv_part_time_title2"
                    android:text="结束时间"
                    android:textColor="#313131"
                    android:textSize="@dimen/dp_14"
                    android:layout_width="wrap_content"
                    android:drawablePadding="@dimen/dp_3"
                    android:drawableLeft="@mipmap/qj_jieshu_time_icon"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    />
                <TextView
                    android:id="@+id/tv_part_time2"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:textColor="@color/black"
                    android:textSize="@dimen/dp_14"
                    android:background="@null"
                    android:gravity="center_vertical"
                    android:layout_toRightOf="@id/tv_part_time_title2"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    />
            </RelativeLayout>

            <View
                android:background="@color/colorText"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_0_5"/>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_35"
                android:layout_marginTop="@dimen/dp_5"
                android:layout_marginBottom="@dimen/dp_5"
                >
                <TextView
                    android:gravity="center"
                    android:id="@+id/tv_part_time_title3"
                    android:text="会议联系人"
                    android:textColor="#313131"
                    android:textSize="@dimen/dp_14"
                    android:layout_width="wrap_content"
                    android:drawablePadding="@dimen/dp_3"
                    android:drawableLeft="@mipmap/meeting_contact_icon"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    />
                <TextView
                    android:id="@+id/tv_part_lianx"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:textColor="@color/black"
                    android:textSize="@dimen/dp_14"
                    android:background="@null"
                    android:gravity="center_vertical"
                    android:layout_toRightOf="@id/tv_part_time_title3"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    />
            </RelativeLayout>

            <View
                android:background="@color/colorText"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_0_5"/>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_35"
                android:layout_marginTop="@dimen/dp_5"
                android:layout_marginBottom="@dimen/dp_5"
                >
                <TextView
                    android:gravity="center"
                    android:id="@+id/tv_part_time_title4"
                    android:text="联系人电话"
                    android:textColor="#313131"
                    android:textSize="@dimen/dp_14"
                    android:layout_width="wrap_content"
                    android:drawablePadding="@dimen/dp_3"
                    android:drawableLeft="@mipmap/meeting_tel_icon"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    />
                <TextView
                    android:id="@+id/tv_part_lianx_phone"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:textColor="@color/black"
                    android:textSize="@dimen/dp_14"
                    android:background="@null"
                    android:gravity="center_vertical"
                    android:layout_toRightOf="@id/tv_part_time_title4"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    />
            </RelativeLayout>

            <View
                android:background="@color/colorText"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_0_5"/>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_35"
                android:layout_marginTop="@dimen/dp_5"
                android:layout_marginBottom="@dimen/dp_5"
                >
                <TextView
                    android:gravity="center"
                    android:id="@+id/tv_part_person_title"
                    android:text="参会人"
                    android:drawablePadding="@dimen/dp_3"
                    android:textColor="#313131"
                    android:drawableLeft="@mipmap/qj_shenpi_icon_qgl"
                    android:textSize="@dimen/dp_14"
                    android:layout_centerVertical="true"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent" />
                <TextView
                    android:id="@+id/tv_part_person"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:textColor="@color/black"
                    android:textSize="@dimen/dp_14"
                    android:layout_centerVertical="true"
                    android:background="@null"
                    android:gravity="center_vertical"
                    android:layout_toRightOf="@id/tv_part_person_title"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text=""
                    />
            </RelativeLayout>
            <View
                android:background="@color/colorText"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_0_5"/>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_35"
                android:layout_marginTop="@dimen/dp_5"
                android:layout_marginBottom="@dimen/dp_5"
                >
                <TextView
                    android:gravity="center"
                    android:id="@+id/tv_part_count_title"
                    android:text="参会人数"
                    android:layout_centerVertical="true"
                    android:drawableLeft="@mipmap/qj_chaosongren_icon_qgl"
                    android:textSize="@dimen/dp_14"
                    android:drawablePadding="@dimen/dp_3"
                    android:textColor="#313131"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent" />
                <TextView
                    android:id="@+id/tv_part_count"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:textColor="@color/black"
                    android:textSize="@dimen/dp_14"
                    android:layout_centerVertical="true"
                    android:editable="false"
                    android:background="@null"
                    android:gravity="center_vertical"
                    android:layout_toRightOf="@id/tv_part_count_title"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    />
            </RelativeLayout>
            <View
                android:background="@color/colorText"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_0_5"/>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_35"
                >
                <TextView
                    android:drawablePadding="@dimen/dp_3"
                    android:drawableLeft="@mipmap/meeting_cont_icon"
                    android:text="会议内容"
                    android:textColor="#313131"
                    android:textSize="@dimen/dp_14"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    />
            </RelativeLayout>
            <!--修改右对齐qgl-->
            <TextView
                android:layout_gravity="left"
                android:id="@+id/et_content"
                android:layout_width="wrap_content"
                android:gravity="top"
                android:textSize="@dimen/sp_12"
                android:padding="@dimen/dp_10"
                android:layout_height="wrap_content" />
        </LinearLayout>
        <RelativeLayout
            android:layout_marginTop="@dimen/dp_10"
            android:layout_width="match_parent"
            android:descendantFocusability="blocksDescendants"
            android:layout_marginLeft="@dimen/dp_10"
            android:layout_height="wrap_content">
            <ImageView
                android:layout_centerVertical="true"
                android:id="@+id/iv_meet_room"
                android:layout_width="@dimen/dp_50"
                android:layout_height="@dimen/dp_50"
                android:background="@mipmap/ic_room"
                />
            <RelativeLayout
                android:layout_toRightOf="@id/iv_meet_room"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/dp_80"
                android:layout_marginLeft="@dimen/dp_10"
                >
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    >
                    <TextView
                        android:id="@+id/tv_meet_room_num"
                        android:text="14号楼1号会议室"
                        android:textColor="@color/black"
                        android:textSize="@dimen/dp_14"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:layout_marginTop="@dimen/dp_10"
                        android:layout_below="@id/tv_meet_room_num"
                        android:id="@+id/tv_meet_count"
                        android:textSize="@dimen/sp_12"
                        android:text="容纳人数：100"
                        android:textColor="#676767"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </RelativeLayout>
            </RelativeLayout>
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/tv_back_instruct"
                android:textSize="@dimen/dp_16"
                android:text="签到"
                android:layout_width="@dimen/dp_260"
                android:layout_height="@dimen/dp_40"
                android:gravity="center"
                android:layout_gravity="center_horizontal"
                android:background="@drawable/edit_background_order"
                android:textColor="@color/white"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="@dimen/dp_20"
                android:layout_centerHorizontal="true"
                />
        </RelativeLayout>
    </LinearLayout>
    </ScrollView>
</LinearLayout>