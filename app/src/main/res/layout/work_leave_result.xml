<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:fresco="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EBEBEB">
    <LinearLayout
        android:id="@+id/work_leave_result_liner"
        android:orientation="vertical"
        android:background="@mipmap/banner_bg_qgl"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_170">

        <RelativeLayout
            android:layout_marginTop="@dimen/dp_10"
            android:layout_width="match_parent"
            android:id="@+id/iv_back"
        android:gravity="center_vertical"
        android:layout_height="@dimen/dp_40">
            <ImageView
                android:layout_marginLeft="@dimen/dp_15"
                android:layout_width="@dimen/dp_12"

                android:layout_centerVertical="true"
                android:background="@mipmap/ic_left_white"
                android:layout_height="@dimen/dp_12" />

            <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:text="请假详情"
        android:textColor="#ffffff"
        android:textSize="@dimen/sp_11" />

        </RelativeLayout>

        <LinearLayout
            android:layout_marginTop="@dimen/dp_15"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <!--开始时间-->
            <LinearLayout
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/tv_start"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="2019/8/8"
                    android:textColor="#ffffffff"
                    android:textSize="@dimen/sp_10"
                    />

                <TextView
                    android:layout_marginTop="@dimen/dp_14"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="开始时间"
                    android:textColor="#ffffffff"
                    android:textSize="@dimen/sp_9"
                    />


            </LinearLayout>
            <!--请假时长-->
            <LinearLayout
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/tv_duration"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="1天"
                    android:textColor="#ffffffff"
                    android:textSize="@dimen/sp_10"
                    />

                <TextView
                    android:layout_marginTop="@dimen/dp_14"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="请假时长"
                    android:textColor="#ffffffff"
                    android:textSize="@dimen/sp_9"
                    />


            </LinearLayout>
            <!--结束时间-->
            <LinearLayout
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/tv_end"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="2019/8/8"
                    android:textColor="#ffffffff"
                    android:textSize="@dimen/sp_10"
                    />

                <TextView
                    android:layout_marginTop="@dimen/dp_14"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="结束时间"
                    android:textColor="#ffffffff"
                    android:textSize="@dimen/sp_9"
                    />


            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

    <ScrollView
        android:layout_marginTop="@dimen/dp_125"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RelativeLayout
            android:layout_marginRight="@dimen/dp_15"
            android:layout_marginLeft="@dimen/dp_15"
            android:background="@drawable/liner_border_gb_qgl"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/iv_remind_my"
                android:layout_width="@dimen/dp_6"
                android:layout_height="@dimen/dp_6"
                android:layout_marginLeft="@dimen/dp_13"
                android:layout_marginTop="@dimen/dp_25"
                android:background="@drawable/liner_yuan_qgl1" />
            <LinearLayout
                android:id="@+id/liner_my_qgl"
                android:layout_marginBottom="@dimen/dp_22"
                android:layout_toLeftOf="@+id/iv_result"
                android:layout_marginLeft="@dimen/dp_10"
                android:layout_marginTop="@dimen/dp_22"
                android:layout_toRightOf="@+id/iv_remind_my"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">


                <!--申请人-->
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/tv_reason_title1"
                        android:text="申请人"
                        android:textColor="#676767"
                        android:textSize="@dimen/sp_9"
                        android:layout_width="@dimen/dp_80"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:id="@+id/tv_leave_shenqingren"
                        android:text="王俊杰"
                        android:textColor="@color/black"
                        android:textSize="@dimen/sp_9"
                        android:layout_toRightOf="@id/tv_reason_title1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </RelativeLayout>

                <!--请假事由-->
                <RelativeLayout
                    android:layout_marginTop="@dimen/dp_20"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                android:id="@+id/tv_reason_title"
                android:text="请假事由"
                android:textColor="#676767"
                android:textSize="@dimen/sp_9"
                android:layout_width="@dimen/dp_80"
                android:layout_height="wrap_content" />
                <TextView
                android:id="@+id/tv_leave_reason"
                android:text="请假事由"
                android:textColor="@color/black"
                android:textSize="@dimen/sp_9"
                android:layout_toRightOf="@id/tv_reason_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
                </RelativeLayout>
                    <!--审批人-->
                <RelativeLayout
                    android:layout_marginTop="@dimen/dp_20"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <TextView
                android:text="审批人"
                android:textSize="@dimen/sp_9"
                android:textColor="#676767"
                android:id="@+id/tv_person_title"
                android:layout_width="@dimen/dp_80"
                android:layout_height="wrap_content" />
                <TextView
                    android:text="张XX"
                android:id="@+id/tv_approved_person"
                android:layout_toRightOf="@id/tv_person_title"
                android:textSize="@dimen/sp_9"
                android:textColor="@color/black"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
                </RelativeLayout>
                 <!--抄送人-->
                <RelativeLayout
                    android:id="@+id/rl_chaosong"
                    android:layout_marginTop="@dimen/dp_20"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <TextView
                        android:text="抄送人"
                        android:textSize="@dimen/sp_9"
                        android:textColor="#676767"
                        android:id="@+id/tv_person_title1"
                        android:layout_width="@dimen/dp_80"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:id="@+id/tv_chaosong_person_qgl"
                        android:layout_toRightOf="@id/tv_person_title1"
                        android:textSize="@dimen/sp_9"
                        android:textColor="@color/black"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </RelativeLayout>
                <!--请假描述-->
                <RelativeLayout
                    android:layout_marginTop="@dimen/dp_20"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <TextView
                        android:text="请假描述"
                        android:textSize="@dimen/sp_9"
                        android:textColor="#676767"
                        android:id="@+id/tv_person_title11"
                        android:layout_width="@dimen/dp_80"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:text="南老家探亲"
                        android:id="@+id/tv_miaoshu"
                        android:layout_toRightOf="@id/tv_person_title11"
                        android:textSize="@dimen/sp_9"
                        android:textColor="@color/black"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </RelativeLayout>


            </LinearLayout>

            <com.facebook.drawee.view.SimpleDraweeView
                fresco:roundedCornerRadius="5dp"
                fresco:actualImageScaleType="fitCenter"
                android:layout_alignTop="@+id/liner_my_qgl"
            android:id="@+id/iv_result"
            android:layout_alignParentRight="true"
            android:layout_width="@dimen/dp_80"
            android:layout_height="@dimen/dp_60"
            android:layout_marginRight="@dimen/dp_20"

            />




        </RelativeLayout>

        <RelativeLayout
            android:layout_marginTop="@dimen/dp_10"
            android:layout_marginRight="@dimen/dp_15"
            android:layout_marginLeft="@dimen/dp_15"
            android:background="@drawable/liner_border_gb_qgl"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_204">

            
            <ImageView
                android:id="@+id/liucheng_img_my"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp_27"
                android:src="@mipmap/liucheng_icon_qgl1"
                android:layout_width="@dimen/dp_20"
                android:layout_height="@dimen/dp_106" />



            <LinearLayout
                android:layout_marginLeft="@dimen/dp_13"
                android:layout_alignTop="@+id/liucheng_img_my"
                android:layout_toRightOf="@+id/liucheng_img_my"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/tv_shenqing"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="提交外出申请"
                    android:textColor="#ff313131"
                    android:textSize="@dimen/sp_9"
                    />

                <TextView
                    android:id="@+id/tv_shenqing_time_qgl"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="2019/8/9 08:00:00"
                    android:textColor="#676767"
                    android:textSize="@dimen/sp_9"
                    />


            </LinearLayout>
            <LinearLayout
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp_13"
                android:layout_toRightOf="@+id/liucheng_img_my"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/tv_shenqingren_qgl"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="[张XX] "
                    android:textColor="#0054CC"
                    android:textSize="@dimen/sp_9"
                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="正在审批中"
                    android:textColor="#ff313131"
                    android:textSize="@dimen/sp_9"
                    />

            </LinearLayout>


            <LinearLayout
                android:layout_marginLeft="@dimen/dp_13"
                android:layout_alignBottom="@+id/liucheng_img_my"
                android:layout_toRightOf="@+id/liucheng_img_my"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/tv_wancheng_qgl"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="完成"
                    android:textColor="#DBDBDB"
                    android:textSize="@dimen/sp_9"
                    />

                <TextView
                    android:id="@+id/tv_wanzcheng_time_qgl"
                    android:visibility="gone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="2019/8/9 08:00:00"
                    android:textColor="#676767"
                    android:textSize="@dimen/sp_9"
                    />


            </LinearLayout>


        </RelativeLayout>
